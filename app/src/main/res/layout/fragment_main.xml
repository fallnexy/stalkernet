<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">

		<ImageView
			android:id="@+id/ImageViewHealth"
			android:layout_width="56dp"
			android:layout_height="56dp"
			android:src="@drawable/healthsymbol2" />

		<ProgressBar
			android:id="@+id/ProgressHealth"
			style="?android:attr/progressBarStyleHorizontal"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="10dp"
			android:scaleY="8.0"
			android:progressTint="@color/common_google_signin_btn_text_dark_pressed"/>


		<ImageView
			android:id="@+id/ImageViewRad"
			android:layout_width="56dp"
			android:layout_height="56dp"
			android:src="@drawable/radsymbol2" />

		<ProgressBar
			android:id="@+id/ProgressRad"
			style="?android:attr/progressBarStyleHorizontal"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="10dp"
			android:scaleY="8.0"
			android:progressTint="@color/common_google_signin_btn_text_dark_pressed"/>

		<ImageView
			android:id="@+id/ImageViewBio"
			android:layout_width="56dp"
			android:layout_height="56dp"
			android:src="@drawable/biosymbol2" />

		<ProgressBar
			android:id="@+id/ProgressBio"
			style="?android:attr/progressBarStyleHorizontal"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="10dp"
			android:scaleY="8.0"
			android:progressTint="@color/common_google_signin_btn_text_dark_pressed"
			android:secondaryProgressTint="@color/common_google_signin_btn_text_dark_pressed"/>

		<ImageView
			android:id="@+id/ImageViewPsy"
			android:layout_width="56dp"
			android:layout_height="56dp"
			android:src="@drawable/psysymbol2" />

		<ProgressBar
			android:id="@+id/ProgressPsy"
			style="?android:attr/progressBarStyleHorizontal"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="10dp"
			android:scaleY="8.0"
			android:progressTint="@color/common_google_signin_btn_text_dark_pressed"/>

		<TextView
			android:id="@+id/txtCoordinates"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:text="text"
			/>

		<TextView
			android:id="@+id/txtMessages"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:text="V 0.27" />

		<TextView
			android:id="@+id/txtVersion"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:text="V 0.27" />
	</LinearLayout>
</LinearLayout>